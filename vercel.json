{
  "buildCommand": "cd packages/frontend && pnpm run build && cp -r ../../docs/site dist/docs",
  "outputDirectory": "packages/frontend/dist",
  "installCommand": "pnpm install",
  "redirects": [
    {
      "source": "/docs",
      "destination": "/docs/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://34.170.58.101:80/api/:path*"
    },
    {
      "source": "/app",
      "destination": "/index.html"
    },
    {
      "source": "/app/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/blog",
      "destination": "/index.html"
    },
    {
      "source": "/blog/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/about",
      "destination": "/index.html"
    },
    {
      "source": "/share/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|docs|_next|static|favicon|assets).*)",
      "destination": "/index.html"
    }
  ]
}
